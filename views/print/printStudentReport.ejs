
    <div class="header">
        <h1><%= orgName %></h1>
        <p>Date: <%= new Date().toLocaleDateString() %></p>
    </div>
    <div class="info">
        <img src="<%= student.image %>" alt="Student Image" style="width:100px; height:auto;">
        <h2><%= student.name %></h2>
    </div>
    <div class="subjects">
        <h3>Enrolled Subjects and Grades</h3>
        <table>
            <tr>
                <th>Subject</th>
                <th>Grade</th>
            </tr>
            <% student.subjects.forEach(function(subject) { %>
                <tr>
                    <td><%= subject.name %></td>
                    <td><%= subject.grade %></td>
                </tr>
            <% }); %>
        </table>
    </div>
    <div class="remarks">
        <label for="teacherRemarks">Teacher's Remarks:</label>
        <textarea id="teacherRemarks"></textarea>
        <label for="headTeacherRemarks">Head Teacher's Remarks:</label>
        <textarea id="headTeacherRemarks"></textarea>
    </div>
    <div class="footer">
        <button onclick="window.print()">Print this report</button>
    </div>

